<%= form_for @photo, url: user_pets_photo_path(@user) , :html =>{:multipart =>true} do |f|%>
  <%=f.error_messages%>
  <p>
    <%=f.label :name%>
    <%=f.text_field :name%>
  </p>
  <p>
    <%=f.file_field :image%>
  </p>
  <p>
    <%=f.label :remote_image_url, "or image URL"%> <br />
    <%=f.text_field :remote_image_url%>
  </p>
  <p><%=f.submit%></p>
<%end%>
